<div class="form">

<%= form_for :artist, url: artists_path do |f| %>

<h1>Adding artists</h1>

 <div class="form-group">
    <%= f.label :Artist %><br>
    <%= f.text_field :artistName, :class => 'form-control' %>
 </div>

 <div class="form-group">
    <%= f.label :Country %><br>
    <%= f.text_field :country, :class => 'form-control' %>
 </div>

 <div class="form-group">
    <%= f.label :Label %><br>
    <%= f.text_field :label, :class => 'form-control' %>
 </div>

 
  <!--<p> 
    <%= f.label :Ano_de_Criação %><br>
